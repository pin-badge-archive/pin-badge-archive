    "prettier": "^1.7.4"
  },
  "dependencies": {
    "babel-plugin-syntax-decorators": "^6.13.0",
    "custom-react-scripts": "0.2.1",
    "firebase": "^4.6.1",
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "mobx": "^3.3.1",
    "mobx-react": "^4.3.3",
    "mobx-react-devtools": "^4.2.15",
    "mobx-react-router": "^4.0.1",
    "moment": "^2.19.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
